{
  "name": "uwa-judge",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node -T -P . --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-docker": "docker build -t tommyjqliu/uwajudge .",
    "tag-docker": "docker tag uwajudge tommyjqliu/uwajudge",
    "push-docker": "docker push tommyjqliu/uwajudge",
    "deprecated-start": "next start",
    "lint": "next lint",
    "test": "docker compose exec webapp npx vitest",
    "db-push": "docker compose exec webapp npx prisma db push",
    "db-migrate": "docker compose exec -u node webapp npx prisma migrate dev",
    "db-migrate-comment": "The user were set for the command so the migration file can be delete without root",
    "db-reset": "docker compose exec webapp npx prisma migrate reset --force",
    "docker-dev-up": "docker compose up -d && docker compose exec webapp npx prisma migrate reset --force",
    "docker-dev-down": "docker compose down",
    "docker-dev-reset": "docker compose down && rm -rf ./.mariadb && rm -rf ./.postgres",
    "docker-pub-up": "export NODE_ENV=production && sh ./scripts/docker-up.sh",
    "docker-pub-down": "export NODE_ENV=production && sh ./scripts/docker-down.sh",
    "docker-pub-reset": "export NODE_ENV=production && sh ./scripts/docker-reset.sh",
    "build-problemtools-web": "docker build -t tommyjqliu/problemtools-web -f ./problemtools/dockerfile ./problemtools",
    "tag-problemtools-web": "docker tag problemtools-web tommyjqliu/problemtools-web",
    "push-problemtools-web": "docker push tommyjqliu/problemtools-web"
  },
  "dependencies": {
    "@azure/msal-node": "^2.6.4",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.15.17",
    "@mui/material": "^5.15.16",
    "@mui/x-date-pickers": "^7.3.2",
    "@prisma/client": "^5.11.0",
    "@tanstack/react-query": "^5.34.2",
    "amqplib": "^0.10.4",
    "archiver": "^7.0.1",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.11",
    "decompress": "^4.2.1",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "monaco-editor": "^0.48.0",
    "next": "^14.2.3",
    "next-auth": "^4.24.7",
    "qs": "^6.12.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.3",
    "tmp": "^0.2.3",
    "zod": "^3.23.4",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/archiver": "^6.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/decompress": "^4.2.7",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.7",
    "@types/qs": "^6.9.15",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/tmp": "^0.2.6",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "next-test-api-route-handler": "^4.0.5",
    "postcss": "^8",
    "prettier": "3.2.5",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  }
}
